{
  "name": "Haruyoshi",
  "description": "Paste in format for site",
  "version": "0.2.2",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "yo.png"
  },
  "icons": {
    "16": "yo.png",
    "48": "yo.png",
    "128": "yo.png"
  },
  "permissions": ["activeTab", "storage", "clipboardRead", "clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ],
  "background": {
    "service_worker": "dist/background.js"
  }
}
